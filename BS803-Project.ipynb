   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import scipy.stats\n",
    "import os"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "11c3da1f-75da-489b-a1bc-79fceb3456e4",
   "metadata": {},
   "outputs": [],
   "source": [
    "def multlinreg(y, x_list, datafile):\n",
    "    if not os.path.exists(datafile):\n",
    "        return False\n",
    "    extension = os.path.splitext(datafile)[1].lstrip(\".\")\n",
    "    if extension != \"csv\": \n",
    "        print('ERROR: FILE MUST BE .CSV.')\n",
    "    else:\n",
    "        data=pd.read_csv(datafile)\n",
    "\n",
    "        # check for given variables\n",
    "        givenvar = [y] + x_list\n",
    "        missing = [var for var in givenvar if var not in data.columns]\n",
    "        if missing: \n",
    "            print(f\"ERROR: Variable(s) {missing} were not found in '{datafile}'\")\n",
    "        else: \n",
    "            # define user-given parameters\n",
    "            y = data[y].values\n",
    "            X = data[x_list].values # matrix of predictor values\n",
    "            n = data.shape[0]\n",
    "            p = len(x_list)\n",
    "            df = n-p-1\n",
    "    \n",
    "            # add intercept column to X matrix\n",
    "            X = np.column_stack((np.ones(n),X))\n",
    "        \n",
    "            # Calculate regression estimates\n",
    "            invxpx = np.linalg.inv(X.T @ X) # (X'X)^-1\n",
    "            xpy = X.T @ y # X'Y\n",
    "            bhat = invxpx @ xpy # Bhat = (X'X)^-1 * X'Y\n",
    "    \n",
    "            # Standard error\n",
    "                # SE = sqrt(var(bhat); var(bhat) = RSS/(n-p-1) * (X'X)^-1\n",
    "            RSS = y.T@y - bhat.T@X.T@y\n",
    "            s2 = RSS/(df)\n",
    "            var = s2 * invxpx\n",
    "            se = np.diag(np.sqrt(abs(var)))\n",
    "    \n",
    "            # Residual standard error\n",
    "            rse = np.sqrt(s2)\n",
    "    \n",
    "            # t-statistic & p-value\n",
    "            tstat = bhat/se\n",
    "            pval = scipy.stats.t.sf(abs(tstat), df)*2\n",
    "        \n",
    "            # R^2 goodness of fit\n",
    "              # SYY = Y'Y - n*(ybar)^2\n",
    "            meany = sum(y) / len(y)\n",
    "            SYY= y.T@y - len(y)*(meany**2)\n",
    "            R2 = 1-(RSS/SYY)\n",
    "    \n",
    "            # Adjusted R^2    \n",
    "            Ra2 = ((n-1)*R2-p)/(df)\n",
    "    \n",
    "            # F statistic\n",
    "            num = (RSS - SYY)/p\n",
    "            den = (RSS/df)\n",
    "            fstat = abs(num/den)\n",
    "            fpval = scipy.stats.f.sf(abs(fstat), (n-1)-df, df)\n",
    "    \n",
    "            # Tabulate all results\n",
    "            x_n = [\"intercept\"] + x_list\n",
    "            table = pd.DataFrame({\"Estimate\": bhat,\n",
    "                                  \"StdError\": se,\n",
    "                                  \"t-statistic\": tstat,\n",
    "                                  \"p-value\": pval}, index=x_n)\n",
    "    \n",
    "            return {\"\": table, \"R2\": R2, \"Adj R2\": Ra2, \"Residual Standard Error\": f\"{rse} on {df} df\",\n",
    "                \"F-statistic\": f\"{fstat} on {p} and {df} df, p-value: {fpval}\"}"
   ]
  },
